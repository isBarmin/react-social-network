{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off",
    "react/no-array-index-key": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "no-case-declarations": "off",
    "react/state-in-constructor": "off"
  },
  "parser": "babel-eslint"
}
